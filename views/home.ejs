<%- include("partials/header") -%>

    <main class="main-home">
        <div class="home-img">
            <img src="https://cdn-icons-png.flaticon.com/256/72/72681.png" alt="img">
        </div>
        <div class="home-head">
            <h3>SECRETS</h3>
        </div>
        <div class="home-para">
            <p>Don't keep your secrets, share them anonymously!</p>
        </div>
        <div class="home-btns">
            <div class="home-register">
                <form action="/" method="post">
                    <button type="submit" name="homeRegister" class="homeRegister">Register</button>
                </form>
            </div>
            <div class="home-login">
                <form action="/" method="post">
                    <button type="submit" class="homeLogin" name="homeLogin">Login</button>
                </form>
            </div>
        </div>
    </main>

<%- include("partials/footer") -%>